{
  "name": "@reciperium/react-recipe",
  "description": "React components for recipe-lang",
  "version": "0.7.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
