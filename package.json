    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.40",
    "rollup-plugin-node-externals": "^7.1.3",
    "storybook": "^8.3.0",
    "storybook-react-context": "^0.7.0",
    "tailwindcss": "^3.4.9",
    "typescript": "5.4.2",
    "vite": "^5.4",
    "vite-plugin-dts": "^4.2.1",
    "vite-plugin-wasm": "^3.3.0"
  }
